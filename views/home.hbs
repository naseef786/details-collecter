<section id="ride-section" class="bg-lite">
   
        <div class="container pt-5">
           
            <h1 class="text-center">welcome to users home</h1>
        <div class="row" style="padding-left: 3.5em;padding-top: 1.5em;" >
          {{#each users}}
        <div class="col-md-4">
            <div class="card text-center p-5" >
               <image class="image" src="{{this.image}}"></image>
                <div class="card-body">
                  <h5 class="card-title">NAME:{{this.name}}</h5>
                 <h6>EMAIL:{{this.email}}</h6>
                 <h6>AGE:{{this.age}}</h6>
                 <h6>PHONE{{this.phone}}</h6>
                 <h6>SPECIFICATION:{{this.studies}}</h6>
                  <a href="/more/{{this.id}}" class="btn btn-primary">view more</a>
                </div>
              </div>
            </div>
             {{/each}}
            </div>
           
            </div>

            
</section>
